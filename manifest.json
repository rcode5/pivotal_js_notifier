{
  "name": "Pivotal Notifier",
  "manifest_version": 2,
  "version": "2.0",
  "description": "Remind me what Pivotal Tracker story I'm working on.",
  "browser_action": { "default_icon": "pivotal_icon.ico"},
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [ "*://*.pivotaltracker.com/*" ],
      "js": ["jquery1.8.3.js","pjsn.js"],
      "run_at": "document_end"
    }
  ]
}
